act-bootstrap\";\nimport \"bootstrap/dist/css/bootstrap.min.css\";\nimport MenuNav from \"../components/menu\";\nimport FrontBar from \"../components/frontbar\";\nimport \"../style/form.css\";\nimport \"../style/frontBar.css\";\n\nexport default function FormUpload() {\n  const history = useHistory();\n  const [url, setURL] = useState(\"\");\n  const [entity, setEntity] = useState(\"\");\n  const [record, setRecord] = useState(\"\");\n  const [topic, setTopic] = useState(\"\");\n  const [area, setArea] = useState(\"\");\n  const [nowDate, setNowDate] = useState(\"\");\n  const [expiredDate, setExpiredDate] = useState(\"\");\n  const [reason, setReason] = useState(\"\");\n  const [others, setOthers] = useState(\"\");\n  const [labels, setLabels] = useState(\"\");\n  const [link, setLink] = useState(\"\");\n  const [file, setFile] = useState(null);\n  const [validated, setValidated] = useState(false);\n\n  const handleEntity = (e) => setEntity(e.currentTarget.value);\n  const handleRecord = (e) => setRecord(e.currentTarget.value);\n  const handleTopic = (e) => setTopic(e.currentTarget.value);\n  const handleArea = (e) => setArea(e.currentTarget.value);\n  const handleNowDate = (e) => setNowDate(e.currentTarget.value);\n  const handleExpiredDate = (e) => setExpiredDate(e.currentTarget.value);\n  const handleReason = (e) => setReason(e.currentTarget.value);\n  const handleOthers = (e) => setOthers(e.currentTarget.value);\n  const handleLabels = (e) => setLabels(e.currentTarget.value);\n  const handleLink = (e) => setLink(e.currentTarget.value);\n\n  function handleUpload(file) {\n    const uploadTask = storage.ref(`oficios/${file.name}`).put(file);\n    uploadTask.on(\"state_changed\", console.log, console.error, () => {\n      storage\n        .ref(\"oficios\")\n        .child(file.name)\n        .getDownloadURL()\n        .then((url) => {\n          setURL(url);\n        });\n    });\n  }\n\n  function handleChange(e) {\n    const file = e.target.files[0];\n    handleUpload(file);\n    setFile(file);\n  }\n\n  // function addDocument(data) {\n  //   db.collection(\"expedientes\").add(data);\n  // }\n\n  const handleSubmit = (event) => {\n    const form = event.currentTarget;\n    if (form.checkValidity() === false) {\n      event.preventDefault();\n      event.stopPropagation();\n    }\n\n    setValidated(true);\n if( \n    entity &&\n    record &&\n    topic &&\n    area &&\n    nowDate &&\n    expiredDate &&\n    reason)\n    {\n    const info = {\n      entidad: entity,\n      expediente: record,\n      tema: topic,\n      Area: area,\n      fecha_entrada: nowDate,\n      fecha_expiracion: expiredDate,\n      motivo: reason,\n      osignermin: others,\n      oefa:  labels,\n      link: link,\n      archivo: url,\n     \n    }\n  \n\n    addUser(info).then((docRef) => {\n      history.push({\n        pathname: \"/home\",\n        state: { userId: docRef.id },\n      });\n    });\n  \n    setEntity(\"\");\n    setRecord(\"\");\n    setReason(\"\");\n    setTopic(\"\");\n    setArea(\"\");\n    setNowDate(\"\");\n    setExpiredDate(\"\");\n    setReason(\"\");\n    setOthers(\"\");\n    setLabels(\"\");\n    setFile(null);\n  }\n  };\n\n  function handleSend() {\n    \n     \n\n    const info = {\n      entidad: entity,\n      expediente: record,\n      tema: topic,\n      Area: area,\n      fecha_entrada: nowDate,\n      fecha_expiracion: expiredDate,\n      motivo: reason,\n      osignermin: others,\n      oefa:  labels,\n      link: link,\n      archivo: url,\n      tareas:[],\n    };\n\n    addUser(info).then((docRef) => {\n      history.push({\n        pathname: \"/home\",\n        state: { userId: docRef.id },\n      });\n    });\n\n    setEntity(\"\");\n    setRecord(\"\");\n    setReason(\"\");\n    setTopic(\"\");\n    setArea(\"\");\n    setNowDate(\"\");\n    setExpiredDate(\"\");\n    setReason(\"\");\n    setOthers(\"\");\n    setLabels(\"\");\n    setFile(null);\n  }\n\n  return (\n    <div className=\"page\">\n      <MenuNav></MenuNav>\n      <div className=\"page-content\">\n        <FrontBar></FrontBar>\n        <Container className=\"container-form\">\n          <h2 className=\"title-form\">AGREGAR DOCUMENTO</h2>\n          <p>Ingresa los datos solicitados y luego presiona GUARDAR</p>\n          <Form validated={validated}>\n            <Form.Row>\n              <Form.Group as={Col} controlId=\"formGridState\">\n                <Form.Label>Entidad</Form.Label>\n                <Form.Control\n                 required\n                  className=\"input-form\"\n                  as=\"select\"\n                  defaultValue=\"Choose...\"\n                  onChange={handleEntity}\n                >\n                  <option>Selecciona...</option>\n                  <option>OSINERGMIN</option>\n                  <option>OEFA</option>\n                  <option>MINEM</option>\n                </Form.Control>\n              </Form.Group>\n\n              <Form.Group as={Col} controlId=\"formGridCity\">\n                <Form.Label>Número de Expediente</Form.Label>\n                <Form.Control\n                  type=\"number\"\n                  value={record}\n                  onChange={handleRecord}\n                  required\n                  className=\"input-form\"\n                />\n              </Form.Group>\n\n              <Form.Group as={Col} controlId=\"formGridTopic\">\n                <Form.Label>Tema</Form.Label>\n                <Form.Control\n                 \n                  value={topic}\n                  onChange={handleTopic}\n                  className=\"input-form\"\n                  required\n                />\n              </Form.Group>\n            </Form.Row>\n\n            <Form.Group controlId=\"exampleForm.ControlTextarea1\">\n              <Form.Label>Motivo</Form.Label>\n              <Form.Control\n                className=\"input-form\"\n                as=\"textarea\"\n                rows={3}\n                value={reason}\n                onChange={handleReason}\n                required\n              />\n              \n            </Form.Group>\n            \n            <Form.Row>\n              <Form.Group as={Col} controlId=\"formGridState\">\n                <Form.Label>Area legal encargada</Form.Label>\n                <Form.Control\n                  as=\"select\"\n                  defaultValue=\"Choose...\"\n                  onChange={handleArea}\n                  className=\"input-form\"\n                  required\n                >\n                  <option>Selecciona...</option>\n                  <option>TGP</option>\n                  <option>COGA</option>\n                </Form.Control>\n              </Form.Group>\n\n              <Form.Group as={Col} controlId=\"formGridCity\">\n                <Form.Label>Fecha de recepción</Form.Label>\n                <Form.Control\n                  type=\"date\"\n                  value={expiredDate}\n                  onChange={handleExpiredDate}\n                  required\n                  className=\"input-form\"\n                />\n              </Form.Group>\n\n              <Form.Group as={Col}>\n                <Form.Label>Fecha de vencimiento</Form.Label>\n                <Form.Control\n                  type=\"date\"\n                  value={nowDate}\n                  onChange={handleNowDate}\n                  required\n                  className=\"input-form\"\n                />\n              </Form.Group>\n            </Form.Row>\n            <Form.Row>\n              <Form.Group as={Col} controlId=\"formGridState\">\n                <Form.Label>Procedimientos OSINERGMIN</Form.Label>\n                <Form.Control\n                  as=\"select\"\n                  defaultValue=\"Choose...\"\n                  onChange={handleOthers}\n                  className=\"input-form\"\n                  required\n                >\n                  <option>Selecciona...</option>\n                  <option>-APORTES POR SU REGULACIÓN</option>\n                  <option>-DERRAME/ FUGA/ SINIESTRO</option>\n                  <option>-INCUMPLIMIENTO DEL EIA</option>\n                  <option>-INFRACCIÓN DE NORMAS DEL SERVICIO</option>\n                  <option>\n                    {\" \"}\n                    -PRESENTACIÓN INFORMACIÓN INCOMPLETA O DEFECTUOSA\n                  </option>\n                </Form.Control>\n                {/* <Form.Control value={others} onChange={handleOthers} required className =\"input-form\"/> */}\n              </Form.Group>\n\n              <Form.Group as={Col} controlId=\"formGridCity\">\n                <Form.Label>Procedimientos OEFA </Form.Label>\n                <Form.Control\n                  as=\"select\"\n                  defaultValue=\"Choose...\"\n                  onChange={handleLabels}\n                  className=\"input-form\"\n                  required\n                >\n                  <option>Selecciona...</option>\n                  <option>-DERRAME/ FUGA/ SINIESTRO</option>\n                  <option>-ALMACENAMIENTO INADEACUADO DE SUSTANCIAS QUIMICAS O RESIDUOS PELIGROSOS</option>\n                  <option>-INCUMPLIMIENTO DEL EIA--LMPs/ECAs DE RUIDO</option>\n                  <option>-INCUMPLIMIENTO DEL EIA --ETAPA DE CONSTRUCCION</option>\n                  <option>-INCUMPLIMIENTO DEL PLAN DE ABANDONO PARCIAl DE LAS LINEAS GN</option>\n                  <option>-INCUMPLIMIENTO DEL PLAN DE MONITOREO AMBIENTAL</option>\n                  <option>OTROS</option>\n                </Form.Control>\n              </Form.Group>\n\n              <Form.Group as={Col} controlId=\"formGridCity\" >\n              <Form.Label>Link del archivo </Form.Label>\n                <Form.Control\n                  value={link}\n                  onChange={handleLink}\n                  required\n                  className=\"input-form\"\n                  />\n              </Form.Group>\n            </Form.Row>\n            <Form.Row>\n              <Form.Group >\n              <Form.File\n                id=\"exampleFormControlFile1\"\n                label=\"Sube el documento\"\n                type=\"file\"\n                onChange={handleChange}\n              />\n              </Form.Group>\n            </Form.Row>\n\n            <Button\n              className=\"btn-form\"\n              variant=\"primary\"\n              // disabled={\n               \n              //   !url ||\n              //   !entity ||\n              //   !record ||\n              //   !topic ||\n              //   !area ||\n              //   !nowDate ||\n              //   !expiredDate ||\n              //   !reason\n              // }\n              onClick={handleSubmit}\n            >\n              Guardar\n            </Button>\n          </Form>\n        </Container>\n      </div>\n    </div>\n  );\n}\n",["93","94"],"/Users/albalucia/Desktop/my-app/src/pages/signUp.jsx",[],"/Users/albalucia/Desktop/my-app/src/controllers/user.js",[],"/Users/albalucia/Desktop/my-app/src/components/listDocument.jsx",[],["95","96"],"/Users/albalucia/Desktop/my-app/src/components/menu.jsx",[],"/Users/albalucia/Desktop/my-app/src/index.js",[],"/Users/albalucia/Desktop/my-app/src/components/priveteRoute.jsx",[],"/Users/albalucia/Desktop/my-app/src/pages/login.jsx",[],"/Users/albalucia/Desktop/my-app/src/components/modalDoc.jsx",[],"/Users/albalucia/Desktop/my-app/src/pages/recover.jsx",[],["97","98"],"/Users/albalucia/Desktop/my-app/src/firebase/firebase.config.js",[],"/Users/albalucia/Desktop/my-app/src/components/modalEditTask.jsx",[],"/Users/albalucia/Desktop/my-app/src/App.js",[],"/Users/albalucia/Desktop/my-app/src/pages/documentDetail.jsx",["99"],"import React, { useContext, useEffect, useState } from \"react\";\nimport { AuthContext } from \"../context/AuthContext\";\nimport ListDocument from \"../components/listDocument\";\nimport { Button, Table } from \"react-bootstrap\";\nimport MydModalWithGrid from \"../components/addTask\";\nimport MenuNav from \"../components/menu\";\nimport \"../style/documentDetail.css\";\nimport FrontBar from \"../components/frontbar\";\nimport ModalAddCont from '../components/modalCont';\nimport { db, getEditTask } from \"../firebase/firebase.config\";\nimport ModalEditTask from \"../components/modalEditTask\";\n\n\n\n\nexport default function DocumentDetail() {\n  const { allDoc } = useContext(AuthContext);\n  const [modalShow, setModalShow] = useState(false);\n  const [modalShow1, setModalShow1] = useState(false);\n  const [showEdit, setEdit] = useState(false);\n \n  const viewDoc = localStorage.getItem(\"file\");\n\n  const document = allDoc.find((doc) => doc.id === viewDoc);\n  const [tareas, setTareas] = useState([]);\n  const idC = document.id;\n\n  useEffect(() => {\n   const colecion = db.collection(\"documents\")\n   colecion.doc(idC)\n      .collection(\"tareas\")\n      .onSnapshot((querySnapshot) => {\n        const arrayData = [];\n        querySnapshot.forEach((doc) => {\n          const data = doc.data();\n          arrayData.push({ ...data, idTask: doc.id });\n        });\n        setTareas(arrayData);\n      });\n    return colecion  \n  }, []);\n\n  \n\n  const handlem = async (e)   => {\n      const doc = await getEditTask(idC, e.currentTarget.id);\n      const post = doc.data();\n      localStorage.setItem('docID', JSON.stringify(post));\n      localStorage.setItem('id', doc.id);\n      console.log(post)\n      setEdit(true);\n    };\n\n    \n  \n   \n  return (\n    <section className=\"documentDetail-container\">\n      <MenuNav />\n\n      <div className=\"document-detail-main\">\n        <FrontBar />\n        <div className=\"documentDetail-container\">\n          {document && (\n            <div className=\"description-document\">\n              <h1 className=\"input-form\">{document.tema}</h1>\n              <ListDocument data={document} />\n              <Table>\n                <tbody>\n                  <tr>\n                    <td>Monto de Contingencia</td>\n          <td className=\"input-form\">s/{document.cantidad} soles - {document.comentario}</td>\n                  </tr>\n                  <tr>\n                    <td>Archivos Adjuntos</td>\n                    <td className=\"input-form\">\n                      {document.archivo && (\n                        <a href={document.archivo} target=\"blank\">\n                          {document.expediente}\n                        </a>\n                      )}\n                      {document.link && (\n                        <a href={document.link} target=\"blank\">\n                          {document.expediente}\n                        </a>\n                      )}\n                    </td>\n                  </tr>\n                </tbody>\n              </Table>\n            </div>\n          )}\n          <div>\n            <Button\n              variant=\"primary\"\n              className=\"add\"\n              onClick={() => setModalShow(true)}\n            >\n              AGREGAR TAREA\n            </Button>\n            <Button variant=\"primary\" className=\"add\" onClick={() => setModalShow1(true)}>\n                    MONTO DE CONTINGENCIA\n            </Button>\n          </div>\n          <MydModalWithGrid\n            show={modalShow}\n            onHide={() => setModalShow(false)}\n            idDoc={document.id}\n            tema={document.tema}\n          />\n        </div>\n        <div className=\"taskDetail-container\">\n        <MydModalWithGrid   show={modalShow} onHide={() => setModalShow(false)}  idDoc={document.id} exp={document.expediente} />\n                <ModalAddCont   show={modalShow1} onHide={() => setModalShow1(false)}  idDoc={document.id} exp={document.expediente} />\n          <Table className=\"table table-bordered\">\n            <thead className=\"header\">\n              <tr>\n                <th>Estado</th>\n                <th>Descripción</th>\n                <th>Área Encargada</th>\n                <th>Fecha</th>\n                <th />\n              </tr>\n            </thead>\n            <tbody className=\"table-body\">\n              {document &&\n                tareas.map((dataTask) => (\n                  <tr key={dataTask.idTask}>\n                    <td className=\"input-form\">{dataTask.completada}</td>\n                    <td className=\"input-form\">{dataTask.descripcion}</td>\n                    <td className=\"input-form\">{dataTask.areaEncargada}</td>\n                    <td className=\"input-form\">{dataTask.fechaPlazo}</td>\n                    <td>\n                   \n                 \n                    <Button  id={dataTask.idTask} variant=\"link\" onClick={handlem}>\n                        Editar\n                      </Button>\n                      <ModalEditTask\n                      key={dataTask.id}\n                        shows={showEdit}\n                        onHides={() => setEdit(false)}\n                        idDocu={document.id}\n                      />\n                    </td>\n            \n                  </tr>\n                ))}\n            </tbody>\n          </Table>\n        </div>\n      </div>\n    </section>\n  );\n}\n","/Users/albalucia/Desktop/my-app/src/components/frontbar.jsx",[],"/Users/albalucia/Desktop/my-app/src/context/AuthContext.js",[],"/Users/albalucia/Desktop/my-app/src/firebase/firestore.js",[],"/Users/albalucia/Desktop/my-app/src/components/toastTask.jsx",[],"/Users/albalucia/Desktop/my-app/src/firebase/auth.js",[],{"ruleId":"100","severity":1,"message":"101","line":25,"column":10,"nodeType":"102","messageId":"103","endLine":25,"endColumn":14},{"ruleId":"100","severity":1,"message":"104","line":116,"column":12,"nodeType":"102","messageId":"103","endLine":116,"endColumn":22},{"ruleId":"105","replacedBy":"106"},{"ruleId":"107","replacedBy":"108"},{"ruleId":"105","replacedBy":"109"},{"ruleId":"107","replacedBy":"110"},{"ruleId":"105","replacedBy":"111"}