n  const [modalShow, setModalShow] = useState(false);\n  const [modalShow1, setModalShow1] = useState(false);\n  const [showEdit, setEdit] = useState(false);\n  const [somo, setM] = useState(\"\");\n \n  const viewDoc = localStorage.getItem(\"file\");\n\n  const document = allDoc.find((doc) => doc.id === viewDoc);\n  const [tareas, setTareas] = useState([]);\n \n\n  useEffect(() => {\n   \n   const colecion = db.collection(\"documents\")\n   colecion.doc(viewDoc)\n      .collection(\"tareas\")\n      .onSnapshot((querySnapshot) => {\n        const arrayData = [];\n        querySnapshot.forEach((doc) => {\n          const data = doc.data();\n          arrayData.push({ ...data, idTask: doc.id });\n        });\n        setTareas(arrayData);\n      });\n    return colecion \n \n  }, []);\n\n  \n\n  const handlem = async (e)   => {\n      const doc = await getEditTask(viewDoc, e.currentTarget.id);\n      const post = doc.data();\n      localStorage.setItem('docID', JSON.stringify(post));\n      localStorage.setItem('id', doc.id);\n      const  idTask = localStorage.getItem(\"id\");\n      setM(idTask)\n      setEdit(true)\n    };\n\n    \n  \n   \n  return (\n    <section className=\"documentDetail-container\">\n      <MenuNav />\n\n      <div className=\"document-detail-main\">\n        <FrontBar />\n        {document && (\n        <div className=\"documentDetail-container\">\n          \n            <div className=\"description-document\">\n              <h1 className=\"input-form\">{document.tema}</h1>\n              <ListDocument data={document} />\n              <Table>\n                <tbody>\n                  <tr>\n                    <td>Monto de Contingencia</td>\n          <td className=\"input-form\">s/{document.cantidad} soles - {document.comentario}</td>\n                  </tr>\n                  <tr>\n                    <td>Archivos Adjuntos</td>\n                    <td className=\"input-form\">\n                      {document.archivo && (\n                        <a href={document.archivo} target=\"blank\">\n                          {document.expediente}\n                        </a>\n                      )}\n                      {document.link && (\n                        <a href={document.link} target=\"blank\">\n                          {document.expediente}\n                        </a>\n                      )}\n                    </td>\n                  </tr>\n                </tbody>\n              </Table>\n            </div>\n          \n        \n          <div>\n            <Button\n              variant=\"primary\"\n              className=\"add fix\"\n              onClick={() => setModalShow(true)}\n            >\n              AGREGAR TAREA\n            </Button>\n            <Button variant=\"primary\" className=\"add fix\" onClick={() => setModalShow1(true)}>\n                    MONTO DE CONTINGENCIA\n            </Button>\n          </div>\n          <MydModalWithGrid\n            show={modalShow}\n            onHide={() => setModalShow(false)}\n            idDoc={viewDoc}\n            tema={document.tema}\n          />\n        </div>\n      )}\n        {document && (\n        <div className=\"taskDetail-container\">\n        <MydModalWithGrid   show={modalShow} onHide={() => setModalShow(false)}  idDoc={viewDoc} exp={document.expediente} />\n                <ModalAddCont   show={modalShow1} onHide={() => setModalShow1(false)}  idDoc={viewDoc} exp={document.expediente} />\n          <Table className=\"table table-bordered\">\n            <thead className=\"header\">\n              <tr>\n                <th>Estado</th>\n                <th>Descripción</th>\n                <th>Área Encargada</th>\n                <th>Fecha</th>\n                <th />\n              </tr>\n            </thead>\n            <tbody className=\"table-body\">\n            \n              {document &&\n                tareas.map((dataTask) => (\n                  <tr key={dataTask.idTask}>\n                    <td className=\"input-form\">{dataTask.completada}</td>\n                    <td className=\"input-form\">{dataTask.descripcion}</td>\n                    <td className=\"input-form\">{dataTask.areaEncargada}</td>\n                    <td className=\"input-form\">{dataTask.fechaPlazo}</td>\n                    <td>\n                   \n                 \n                    <Button  id={dataTask.idTask}  name={dataTask.completada} variant=\"link\" onClick={handlem}>\n                        Editar\n                      </Button>\n                      {somo !==\"\" ? (\n                      <ModalEditTask\n                      key={dataTask.id}\n                        shows={showEdit}\n                        onHides={() => setEdit(false)}\n                        idDocu={viewDoc}\n                      />\n                      ): null}\n                    </td>\n            \n                  </tr>\n                ))}\n            </tbody>\n          </Table>\n\n        </div>\n        )}\n      </div>\n    </section>\n  );\n                }\n","/Users/usuario/Desktop/progbasic/Laboratoria/TGP/src/reportWebVitals.js",[],"/Users/usuario/Desktop/progbasic/Laboratoria/TGP/src/pages/signUp.jsx",[],"/Users/usuario/Desktop/progbasic/Laboratoria/TGP/src/components/modalCont.jsx",[],"/Users/usuario/Desktop/progbasic/Laboratoria/TGP/src/components/priveteRoute.jsx",[],"/Users/usuario/Desktop/progbasic/Laboratoria/TGP/src/firebase/auth.js",[],"/Users/usuario/Desktop/progbasic/Laboratoria/TGP/src/components/toastTask.jsx",[],"/Users/usuario/Desktop/progbasic/Laboratoria/TGP/src/components/listDocument.jsx",[],"/Users/usuario/Desktop/progbasic/Laboratoria/TGP/src/pages/recover.jsx",[],{"ruleId":"75","replacedBy":"76"},{"ruleId":"77","replacedBy":"78"},{"ruleId":"79","severity":1,"message":"80","line":44,"column":6,"nodeType":"81","endLine":44,"endColumn":8,"suggestions":"82"},"no-native-reassign",["83"],"no-negated-in-lhs",["84"],"react-hooks/exhaustive-deps","Re